package com.company.model;

public class Produto {
    public String nome;
    public double preco;

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public double getPreco() {
        return preco;
    }

    public void setPreco(double preco) {
        this.preco = preco;
    }

    @Override
    public String toString() {
        return "\nProduto:" + nome  +
                "\nPre√ßo:" + preco;
    }
}
